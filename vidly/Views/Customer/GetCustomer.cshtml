@model vidly.Models.Customer

@{
    ViewBag.Title = "GetCustomer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customer Name</h2>
<hr />

<p>@Model.Name</p>
<ul>
    <li>Membership Type: @Model.membershipType.Name</li>
    @{ 
        if(Model.birthdate != null)
        {
            <li>Birthdate: @Model.birthdate</li>
        }
    }
</ul>
